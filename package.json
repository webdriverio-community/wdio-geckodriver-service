{
  "name": "wdio-geckodriver-service",
  "version": "2.1.2",
  "description": "WebdriverIO service to start & stop GeckoDriver",
  "author": "Rafal Skorka",
  "license": "MIT",
  "homepage": "https://github.com/RafalSkorka/wdio-geckodriver-service#readme",
  "main": "src/index.js",
  "keywords": [
    "wdio",
    "geckodriver",
    "webdriver",
    "wdio-plugin",
    "wdio-service"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RafalSkorka/wdio-geckodriver-service.git"
  },
  "bugs": {
    "url": "https://github.com/RafalSkorka/wdio-geckodriver-service/issues"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "build": "tsc -b .",
    "ci": "run-s test:lint build test",
    "clean": "rimraf ./node_modules package-lock.yaml ./dist",
    "release": "release-it --github.release",
    "release:ci": "npm run release -- --ci --npm.skipChecks --no-git.requireCleanWorkingDir",
    "release:patch": "npm run release -- patch",
    "release:minor": "npm run release -- minor",
    "release:major": "npm run release -- major",
    "test": "run-s test:*",
    "test:lint": "eslint src test",
    "test:e2e": "wdio run ./test/wdio.conf.ts",
    "watch": "npm run build -- --watch"
  },
  "devDependencies": {
    "@types/node": "^18.0.6",
    "@types/split2": "^3.2.1",
    "@types/tcp-port-used": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "@wdio/cli": "^7.19.2",
    "@wdio/local-runner": "^7.19.2",
    "@wdio/mocha-framework": "^7.19.2",
    "@wdio/types": "^7.20.7",
    "eslint": "^8.20.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-base-typescript": "^1.1.0",
    "eslint-import-resolver-typescript": "^3.2.7",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-wdio": "^7.20.6",
    "geckodriver": "^3.0.1",
    "husky": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "release-it": "^15.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "geckodriver": "^3.0.1"
  },
  "dependencies": {
    "@wdio/logger": "^7.19.0",
    "fs-extra": "^10.0.1",
    "get-port": "^5.1.1",
    "split2": "^4.1.0",
    "tcp-port-used": "^1.0.2"
  }
}
